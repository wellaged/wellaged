{
  "name": "wellaged",
  "version": "0.0.4",
  "description": "WellAgEd Editor",
  "main": "dist/index.html",
  "homepage": "https://github.com/wellaged/wellaged#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wellaged/wellaged.git"
  },
  "bugs": "https://github.com/wellaged/wellaged/issues",
  "author": {
    "name": "Andreas C. Osowski",
    "email": "andreas@osowski.de"
  },
  "readmeFilename": "README.md",
  "license": "Apache-2.0",
  "scripts": {
    "test": "karma start",
    "test:watch": "karma start --no-single-run",
    "build": "gulp build",
    "docs": "npm run sassdocs && npm run jsdocs",
    "start": "gulp",
    "buildjs": "jspm bundle-sfx wellaged/api dist/wellaged.min.js --skip-source-maps --minify",
    "jsdocs": "esdoc -c .esdoc.json",
    "sassdocs": "sassdoc src/scss"
  },
  "keywords": [
    "argumentation",
    "argument",
    "graph",
    "editor",
    "carneades"
  ],
  "directories": {
    "doc": "docs",
    "test": "test",
    "baseURL": "src",
    "packages": "src/lib"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-as-promised": "^5.2.0",
    "del": "^2.2.0",
    "esdoc": "^0.4.6",
    "eslint": "^2.3.0",
    "eslint-config-standard": "^5.1.0",
    "eslint-plugin-standard": "^1.3.2",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-cached": "^1.1.0",
    "gulp-concat": "^2.6.0",
    "gulp-connect": "^3.1.0",
    "gulp-eslint": "^2.0.0",
    "gulp-imagemin": "^2.4.0",
    "gulp-minify-css": "^1.2.4",
    "gulp-minify-html": "^1.0.6",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.5.4",
    "gulp-sass": "^2.2.0",
    "gulp-scss-lint": "^0.3.9",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^1.5.3",
    "gulp-util": "^3.0.7",
    "imagemin-pngquant": "^4.2.2",
    "jspm": "^0.16.30",
    "karma": "^0.13.21",
    "karma-chai": "^0.1.0",
    "karma-chai-as-promised": "^0.1.2",
    "karma-chrome-launcher": "^0.2.2",
    "karma-jspm": "^2.0.2",
    "karma-mocha": "^0.2.2",
    "karma-mocha-reporter": "^2.0.0",
    "mocha": "^2.4.5",
    "require-dir": "^0.3.0",
    "run-sequence": "^1.1.5",
    "sassdoc": "^2.1.20"
  },
  "jspm": {
    "directories": {
      "baseURL": "src",
      "doc": "docs",
      "packages": "src/lib",
      "test": "test"
    },
    "dependencies": {
      "backbone": "npm:backbone@^1.2.3",
      "css": "github:systemjs/plugin-css@^0.1.27",
      "jointjs": "npm:jointjs@^0.9.5",
      "js-yaml": "npm:js-yaml@^3.4.6",
      "klayjs": "npm:klayjs@^0.3.2"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.8.24",
      "babel-runtime": "npm:babel-runtime@^5.8.24",
      "clean-css": "npm:clean-css@^3.4.9",
      "core-js": "npm:core-js@^1.1.4"
    }
  }
}
